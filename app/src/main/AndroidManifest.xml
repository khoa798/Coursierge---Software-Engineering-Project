<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jhacks.vrclassroom">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Student"
            android:label="Join a Session " />
        <activity
            android:name=".Professor"
            android:label="Start a Session" />
        <activity
            android:name=".StudSession"
            android:label="Watching" />
        <activity
            android:name=".ProfSession"
            android:label="Streaming" />
        <activity android:name=".ProfMenu" />
        <activity android:name=".AskQuestion"></activity>
    </application>

</manifest>